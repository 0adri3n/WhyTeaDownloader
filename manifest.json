{
    "manifest_version": 2,
    "name": "Why Tea Downloader",
    "description": "Quick YouTube Downloader Extension", 
    "version": "1.0", 
   "permissions": [
    "http://*/*",
    "https://*/*",
       "tabs", 
       "notifications",
    "storage"
   ],
    "browser_action": {  
     "default_icon" : "img/icon.png",
     "default_popup": "popup.html" 
    },
    "icons": { 
     "128" : "img/icon.png"
    },
     "content_scripts": [
       {
         "matches": ["http://*/*"],
         "css": ["style.css"],
         "js": ["popup.js"],
         "all_frames": true
       }
     ]
   }